{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "format": { "outputMode": "new-only" },
    "check": {
      "dependsOn": ["format"],
      "outputMode": "new-only"
    },
    "build": {
      "dependsOn": ["format"],
      "outputMode": "new-only"
    },
    "test": {
      "dependsOn": ["build"],
      "cache" : false
    }
  }
}
