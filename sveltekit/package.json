{
  "name": "autoswarm-frontend",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "all": "pnpm clean && pnpm install && pnpm format && pnpm build && pnpm test-run",
    "clean": "rm -rf node_modules .sveltkit build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write .",
    "build": "vite build",
    "test": "vitest",
    "test-run": "vitest --run",
    "test2": "npm run test:integration && npm run test:unit",
    "dev": "vite dev",
    "preview": "vite preview",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.27.4",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "undici": "^5.27.2",
    "viem": "^1.18.9",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "type": "module"
}