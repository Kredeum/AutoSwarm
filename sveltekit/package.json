{
	"devDependencies": {
		"@fairdatasociety/bmt-js": "^2.1.0",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/node": "^20.11.16",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"esbuild-jest": "^0.5.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.10",
		"svelte-check": "^3.6.3",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"viem": "^2.7.6",
		"vite": "^5.0.12",
		"vitest": "^1.2.2"
	},
	"name": "@autoswarm/dapp",
	"private": true,
	"scripts": {
		"all": "pnpm clean && pnpm install && pnpm format && pnpm build && pnpm test-run",
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"clean": "rm -rf node_modules .sveltkit build",
		"dev": "vite dev",
		"format": "prettier --write .",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"preview": "vite preview",
		"test": "vitest --run",
		"test-watch": "vitest",
		"test2": "npm run test:integration && npm run test:unit"
	},
	"type": "module",
	"version": "0.0.5",
	"dependencies": {
		"@autoswarm/common": "workspace:^",
		"@autoswarm/components": "workspace:^",
		"@autoswarm/contracts": "workspace:^"
	}
